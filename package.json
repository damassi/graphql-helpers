{
  "name": "graphql-helpers",
  "version": "0.0.1",
  "description": "Tools and utilities for making GraphQL servers easier to build",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "build:parsers": "./node_modules/.bin/jison ./grammars/graphql_type_grammar.jison -o ./src/parsers/typeParser.js",
    "test": "cross-env BABEL_ENV=commonjs ava",
    "test:watch": "cross-env BABEL_ENV=commonjs ava --watch"
  },
  "ava": {
    "files": [
      "src/**/__tests__/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "verbose": true,
    "require": "babel-register",
    "babel": "inherit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-stage-1": "^6.5.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.12.0",
    "graphql": "^0.6.0",
    "graphql-relay": "^0.4.2",
    "jison": "^0.4.17",
    "rimraf": "^2.5.2",
    "standard-react": "^2.0.1",
    "stylelint": "^6.5.1",
    "webpack": "^1.13.1"
  }
}
